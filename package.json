{
  "name": "b10rl8w",
  "version": "1.0.0",
  "description": "Central impl repository for our RL project",
  "main": "index.js",
  "scripts": {
    "precommit": "npm run test && npm run prettier",
    "dev": "parcel ./src/index.html -d build/",
    "start-dev": "ELECTRON_START_URL=http://localhost:1234 electron ./src",
    "start": "electron .",
    "test": "jest",
    "prettier": "prettier **/*\\.(js|ts|tsx|jsx)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuncanWalter/B10RL8W.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DuncanWalter/B10RL8W/issues"
  },
  "homepage": "https://github.com/DuncanWalter/B10RL8W#readme",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 2
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "husky": "0.14.3",
    "jest": "23.5.0",
    "parcel-bundler": "1.9.7",
    "prettier": "1.14.2",
    "ts-jest": "23.1.4",
    "typescript": "3.0.3"
  },
  "dependencies": {
    "electron": "2.0.8",
    "electron-reload": "^1.2.5",
    "gpu.js": "1.7.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
